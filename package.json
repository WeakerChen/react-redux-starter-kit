{
  "name": "react-redux-starter-kit",
  "version": "0.2.0",
  "description": "React + Redux starter kit for frontend, including simple backend structure.",
  "main": "server.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "watch": "npm run watch-fe & npm run watch-be",
    "build-dev": "npm run build-fe-dev & npm run build-be-dev",
    "build-prod": "",
    "watch-fe": "npm run clean-fe && ./node_modules/webpack/bin/webpack.js --watch --config webpack/dev.config.js",
    "build-fe-dev": "webpack --config webpack/dev.config.js",
    "build-fe-prod": "rm -rf ./build/* && webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "build-fe-profiling": "webpack --verbose --colors --profiles --display-modules --display-error-details --config webpack/prod.config.js",
    "clean-fe": "rm -rf ./client/build/*",
    "watch-be": "nodemon server/app.js --config server/nodemon.json",
    "build-be-dev": "",
    "build-be-prod": "",
    "lint": "eslint -c .eslintrc js/app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanselfmu/react-redux-starter-kit.git"
  },
  "author": "Changhao Han",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hanselfmu/react-redux-starter-kit/issues"
  },
  "homepage": "https://github.com/hanselfmu/react-redux-starter-kit#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "boom": "^4.2.0",
    "express": "^4.14.0",
    "flux-standard-action": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "keymirror": "^0.1.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "multer": "^1.2.0",
    "mz": "^2.6.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "node-sass": "^3.13.1",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.0",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.3.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.27"
  }
}
